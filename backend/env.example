# Database (PostGIS required - enable in Supabase Dashboard > Database > Extensions)
DATABASE_URL=postgresql://postgres:password@db.xxxxx.supabase.co:5432/postgres

# Parking Lot Data Sources
# INRIX - Get from https://iq.inrix.com (My Key section)
INRIX_APP_ID=your_inrix_app_id
INRIX_HASH_TOKEN=your_inrix_hash_token

# HERE - Get from https://developer.here.com
HERE_API_KEY=your_here_api_key

# Google APIs - Get from https://console.cloud.google.com
# Used for: Business contact data (Places API) + Satellite imagery (Static Maps API)
GOOGLE_PLACES_KEY=your_google_places_api_key
GOOGLE_MAPS_KEY=your_google_maps_api_key

# Regrid Parcel Data - Get from https://regrid.com
# Tileserver API for parcel geometries (tiles are free, unlimited)
REGRID_TILESERVER_TOKEN=your_regrid_token

# Object Storage (optional - for storing satellite images)
# Option A: Supabase Storage
SUPABASE_STORAGE_URL=https://xxxxx.supabase.co/storage/v1
SUPABASE_STORAGE_KEY=your_supabase_storage_key
# Option B: AWS S3
# AWS_S3_BUCKET=your_bucket_name
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret
# AWS_REGION=us-east-1

# Roboflow API (for parking lot condition analysis)
ROBOFLOW_API_KEY=your_roboflow_api_key
ROBOFLOW_MODEL_ID=pavement-crack-detection-r4n7n/1

# OpenAI API (for GPT-4o Vision lead scoring)
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key

# Security
SECRET_KEY=generate_with_openssl_rand_hex_32

# Environment
ENVIRONMENT=development
